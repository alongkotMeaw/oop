package lap2;

public class ex2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		final int SIZE = 5;
		
		int[][] grid = new int[SIZE][SIZE];
		for (int i = 0; i < SIZE; i++) {
			for (int j = 0; j < SIZE; j++) {
				grid[i][j] = i * SIZE +j +1;
				System.out.print(grid);
			}
			System.out.println();
		}
	}

}
