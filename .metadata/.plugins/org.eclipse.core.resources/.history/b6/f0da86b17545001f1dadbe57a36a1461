package lap4;

public class main_BankAccout {

	public static void main(String[] args) {
		Date day = new Date();
		BankAccount ac1 = new BankAccount("accout");
		// set day
		day.setter(1, 11, 2015);
		//ac1.string_getinfo();
		//ac1.property();
		BankAccount ac2 = new BankAccount("alongkot", 500, day);
		//ac2.string_getinfo();
		//ac2.property();
		
		ac1.deposit(1000);
		ac2.tranfer(ac1, 300);
		if(ac2.withdraw(300)) System.out.println("out of amount");
		ac1.string_getinfo();
		ac1.property();
		System.out.println("----------------------------");
		ac2.string_getinfo();
		ac2.property();
		
		

	}

}
